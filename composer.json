{
  "name": "passionweb/form-spamshield",
  "license": "GPL-3.0",
  "description": "Based on wsm/form-spamshield. Protects forms from spam by using an alternative solution to captchas.",
  "type": "typo3-cms-extension",
  "homepage": "https://www.passionweb.de",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Manuel Schnabel",
      "role": "Developer"
    }
  ],
  "require": {
    "typo3/cms-core": "^11.5 | ^12.4",
    "wsm/form-spamshield": "^1.3"
  },
  "replace": {
      "typo3-ter/form-spamshield": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "form_spamshield"
    }
  }
}
